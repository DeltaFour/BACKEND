<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DeltaFour.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DeltaFour.Maui"
    xmlns:pages="clr-namespace:DeltaFour.Maui.Pages"
    Shell.FlyoutBehavior="Disabled"
    Shell.TabBarBackgroundColor="#F5F8FB"
    Shell.TabBarTitleColor="#1e2d69"
    Title="DeltaFour.Maui">
        <ShellContent
        Title="L\/"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <TabBar>
        <ShellContent
         Title="\H\"
         ContentTemplate="{DataTemplate pages:EmployeResume}"
         Route="EmployeResumePage" />
        <ShellContent
       Title="\A"
       ContentTemplate="{DataTemplate pages:FaceRegisterPage}"
       Route="FaceRegisterPage" />

    </TabBar>
</Shell>

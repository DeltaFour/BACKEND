<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DeltaFour.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DeltaFour.Maui"
    xmlns:pages="clr-namespace:DeltaFour.Maui.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="DeltaFour.Maui">
    <TabBar>
        <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        <ShellContent
         Title="Home"
         ContentTemplate="{DataTemplate pages:EmployeResume}"
         Route="EmployeResumePage" />
        <ShellContent
       Title="AuthFace"
       ContentTemplate="{DataTemplate pages:FaceRegisterPage}"
       Route="FaceRegisterPage" />

    </TabBar>
</Shell>
